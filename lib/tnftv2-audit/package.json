{
  "name": "tangible-nft",
  "version": "2.0.0",
  "description": "TangibleNFT Marketplace V2",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "start": "npx hardhat deploy",
    "start:mumbai": "npx hardhat deploy --network mumbai --gasprice 20000000000",
    "start:rinkeby": "npx hardhat run scripts/deployAll.js --network rinkeby",
    "prettier": "prettier --write --plugin=prettier-plugin-solidity '**/*.{sol,md}'",
    "prettierOne": "prettier --write --plugin=prettier-plugin-solidity ",
    "lint": "prettier --list-different 'contracts/**/*.sol'"
  },
  "author": "TNFT Limited",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "2.3.3",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.3",
    "canonical-weth": "^1.4.0"
  },
  "devDependencies": {
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "axios": "^1.4.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs": "^0.0.1-security",
    "hardhat": "^2.16.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.34",
    "hardhat-tracer": "^2.5.1",
    "json-aggregate": "^0.1.2",
    "mocha-skip-if": "^1.0.4",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2"
